<Application x:Class="ExaminationTickets.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ExaminationTickets"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/TextBlockDictionary.xaml" />
                <ResourceDictionary Source="Theme/TextBoxDictionary.xaml" />
                <ResourceDictionary Source="Theme/ButtonDictionary.xaml" />
                <ResourceDictionary Source="Theme/SwitchDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
